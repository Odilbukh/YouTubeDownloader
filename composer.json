    "homepage": "https://iwannacode.net",
    "license": "MIT",
    "authors": [
        {
            "name": "Oleksandr Pinchuk",
            "email": "intracomof@gmail.com",
            "homepage": "https://iwannacode.net",
            "role": "Developer"
        }
